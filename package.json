{
  "name": "nextjs-revalidate",
  "version": "1.6.6",
  "description": "SwissCaution fork of SuperHuit's Next.js plugin allows you to purge & re-build the cached pages from the WordPress admin area. It also automatically purges & re-builds when a page/post/... is save or updated.",
  "main": "src/index.ts",
  "repository": "https://github.com/SwissCautionSA/nextjs-revalidate.git",
  "author": "superhuit x SwissCaution <informatique@swisscaution.ch>",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "yarn start & webpack --mode development --watch",
    "start": "wp-env start --xdebug && yarn start:revalidateServer",
    "stop": "wp-env stop",
    "start:revalidateServer": "node ./config/revalidate-server.js"
  },
  "devDependencies": {
    "@wordpress/env": "^9.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.9.1",
    "express": "^4.18.2",
    "mini-css-extract-plugin": "^2.7.7",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
